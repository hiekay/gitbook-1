{
  "gitbook": "3.2.2",
  "title": "python基础教程",
  "description": "慕课网python基础教程中文版",
  "language": "zh",
  "structure": {
    "readme": "README.md"
  },
  "pluginsConfig": {
    "fontSettings": {
      "theme": "white",
      "family": "./fonts/dnks.ttf",
      "size": 2
    },
    "plugins": [
      "yahei",
      "katex",
      "-search"
    ]
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 18,
    "paperSize": "a4",
    "margin": {
      "right": 30,
      "left": 30,
      "top": 30,
      "bottom": 50
    }
  }
}
